<view class="goods_wrap">
    <navigator url="/pages/goodsdetail/goodsdetail?goods_id={{item.goods_id}}"
    wx:for="{{collectList}}"
    wx:key="goods_id"
    >
        <view class="goods_img_wrap">
            <image src="{{item.goods_small_logo?item.goods_small_logo:'https://ww1.sinaimg.cn/large/007rAy9hgy1g24by9t530j30i20i2glm.jpg'}}" mode="widthFix"/>
        </view>
        <view class="goods_text_wrap">
            <view class="goods_name">{{item.goods_name}}</view>
            <view class="goods_price">ï¿¥{{item.goods_price}}</view>
        </view>
    </navigator>
</view>